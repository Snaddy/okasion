<div class="profile">

<div class="info">
	<strong>Name: </strong><%= @name %><br>
	<strong>City: </strong><%= @city %>

	<%= link_to edit_user_path, class: 'btn btn-primary' do%>
 		Edit Profile <span class="fa fa-pencil"></span>
 	<% end %>
</div>

<div class="feed">


<hr>

<div class="events">
	<%= render @events %>
</div>

<div class="pagination">
	<%= will_paginate @events %>
</div>

<% if @events.next_page %>
	<div id="load-more-posts" class="btn btn-primary"> Load more...</div>
<% end %> 
</div>
</div>